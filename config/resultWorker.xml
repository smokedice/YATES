<resultWorker
    xmlns="resultWorker"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="resultWorker validation/resultWorker.xsd">
    <output location="/tmp/results">
        <sqlLite enabled="true" clear="true" />
        <logFile enabled="true" clear="true" />
        <csvFile enabled="true" clear="true" />
        <fileSystem enabled="true">
            <clear>true</clear>
            <archive>
                <name>resultsPackage</name>
                <format>tar.gz</format>
                <location>/tmp/results</location>
            </archive>
        </fileSystem>
        <trmsLoggers>
            <trms enabled="false">
                <url>https://devint-trms/trms</url>
                <username>blah</username>
                <password>blah</password>
            </trms>
            <trms enabled="false">
                <url>https://devint-trms/trms</url>
                <username>blah</username>
                <password>blah</password>
            </trms>
        </trmsLoggers>
    </output>
    <customLogFilters></customLogFilters>
</resultWorker>
